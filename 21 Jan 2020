Enumuration
==============================================
enum demoenum
{
  case sun, cloud, foggy, rain 
}
var demo=demoenum.sun
print(demo)

===============================================
print star Pattern:
===============================================
print("Enter the number of rows: ")
var input = Int(readLine()!)!
print("\nOutput is : \n")
for i in 1...input
{
  for j in 1...i
  {
      print("*",terminator:" ")
  }
  print("\n")
}

